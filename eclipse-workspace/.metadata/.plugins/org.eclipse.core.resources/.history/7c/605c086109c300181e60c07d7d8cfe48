package package3;

public class IfandElse {

	public static void main(String[] args) {
			int i=5,j = 1;
			if (i==1) {
				if (i<=5) {
					System.out.println("Hello world");
					if (j==1) {
						{
							if (j<i) {
								System.out.println("Bye");
							}
						}
					}
				}
				
			}
			else {
				System.out.println("Get Lost");
			}
	}

}
